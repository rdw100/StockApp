{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [ "/_framework/*", "/css/*" ]
  },
  "platform": {
    "apiRuntime": "dotnet-isolated:8.0"
  },
  "routes": [
    {
      "route": "/anonymous/*",
      "allowedRoles": [ "anonymous" ]
    },
    {
      "route": "/authenticated/*",
      "allowedRoles": [ "authenticated" ]
    },
    {
      "route": "/api/quote/*",
      "allowedRoles": [ "anonymous" ]
    },
    {
      "route": "/counter/",
      "allowedRoles": [ "authenticated" ]
    },
    {
      "route": "/weather/*",
      "allowedRoles": [ "authenticated" ]
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/github?post_login_redirect_uri=.referrer",
      "statusCode": 302
    }
  }
}