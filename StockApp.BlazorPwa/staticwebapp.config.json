{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [ "/_framework/*", "/css/*" ]
  },
  "platform": {
    "apiRuntime": "dotnet-isolated:8.0"
  },
  "routes": [
    {
      "route": "/anonymous/*",
      "allowedRoles": [ "anonymous" ]
    },
    {
      "route": "/authenticated/*",
      "allowedRoles": [ "authenticated" ]
    },
    {
      "route": "/api/quote/*",
      "allowedRoles": [ "authenticated" ]
    },
    {
      "route": "/counter",
      "allowedRoles": [ "anonymous" ]
    },
    {
      "route": "/weather",
      "allowedRoles": [ "authenticated" ]
    }
  ]
}